-- Queries
SELECT * FROM Students;
SELECT name, marks FROM Students WHERE marks &gt; 80 ORDER BY marks DESC;
SELECT * FROM Students WHERE city IN ('Pune','Mumbai');
SELECT name FROM Students WHERE name LIKE 'P%';
SELECT name, marks FROM Students ORDER BY marks DESC LIMIT 3;
